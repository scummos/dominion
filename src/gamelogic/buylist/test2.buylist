buylist
    list if AnyOf(SupplyHasLess(Province, 2), SupplyEmptyPilesGreater(1))
        Province, Duchy, Estate
    end
    list if SupplyHasLess(Province, 4)
        Province, Duchy
    end
    list
        Province if Has(Gold, 1),
        Witch if HasExact(Witch, 0),
        Gold,
        Witch if HasExact(Witch, 1),
        Silver
    end
end
